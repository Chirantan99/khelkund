<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.appacitive.khelkund.fragments.pick5.Pick5UpcomingFragment">


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/iv_match_over_flag"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:src="@drawable/timer" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="COME BACK LATER"
        android:id="@+id/tv_match_over_label"
        android:layout_below="@+id/iv_match_over_flag"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="This match is not yet open."
        android:id="@+id/textView31"
        android:layout_below="@+id/tv_match_over_label"
        android:layout_centerHorizontal="true" />
</RelativeLayout>
